<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="io.github.joseseie.projectoitent.MainActivity">


    <Button
        android:id="@+id/btnChamarBroswer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/lbl_chamar_browser" />

    <Button
        android:id="@+id/btnFazerLigacao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/lbl_fazer_ligacao" />

    <Button
        android:id="@+id/btnExemploParametro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/lbl_exemplo_parametro" />
</LinearLayout>
